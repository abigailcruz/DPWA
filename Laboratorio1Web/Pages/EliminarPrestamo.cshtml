@page
@model Laboratorio1Web.Pages.EliminarPrestamoModel
@{
}

<h1>Eliminar Prestamo</h1>
<form method="post">
    <input type="hidden" asp-for="Prestamo.Id" />
    <div asp-validation-summary="ModelOnly"> </div>
    <div class="form-group">
        <label asp-for="Prestamo.CodigoPrestamo">
            <input type="text" class="form-control" asp-for="Prestamo.CodigoPrestamo"  readonly="readonly">
        </label>
       
    </div>
    <div class="form-group">
        <label asp-for="Prestamo.FechaPrestamo">
            <input type="text" class="form-control" asp-for="Prestamo.FechaPrestamo"   readonly="readonly">
        </label>
        
    </div>
    <div class="form-group">
        <label asp-for="Prestamo.FechaDevolucion">
            <input type="text" class="form-control" asp-for="Prestamo.FechaDevolucion"  readonly="readonly" >
        </label>
      
    </div>
    <div class="form-group">
        <label asp-for="Prestamo.ClienteID">
            <input type="text" class="form-control" asp-for="Prestamo.ClienteID"  readonly="readonly" >
        </label>
       
    </div>
    <div class="form-group">
        <label asp-for="Prestamo.PeliculaID ">
            <input type="text" class="form-control" asp-for="Prestamo.PeliculaID "  readonly="readonly">
        </label>
     
    </div>

    <button type="submit" value="Delete" class="btn btn-danger">Eliminar</button> <br />
    <a asp-page="/Prestamos">Regresar a Prestamos</a>
</form>